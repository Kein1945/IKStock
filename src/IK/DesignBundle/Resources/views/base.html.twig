<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8">
    <title>{% block page_title %}!set value of page_title block{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Le styles -->
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    {% stylesheets '@IKDesignBundle/Resources/public/bootstrap/css/bootstrap.min.css'
        '@IKDesignBundle/Resources/public/bootstrap/css/bootstrap-responsive.css'
        '@IKDesignBundle/Resources/public/google/prettify.css'%}
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
    {% endstylesheets %}
    {% block stylesheets %}{% endblock %}
    {% javascripts '@IKDesignBundle/Resources/public/jquery/jquery.min.js' %}
        {# http://html5shim.googlecode.com/svn/trunk/html5.js #}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
